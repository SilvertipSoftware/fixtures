{
    "name": "silvertipsoftware/fixtures",
    "description": "A better fixture package for Laravel",
    "keywords": ["laravel", "fixtures"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Silvertip Software",
            "email": "info@silvertipsoftware.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "illuminate/support": "5.4.*",
        "illuminate/database": "5.4.*",
        "illuminate/filesystem": "5.4.*",
        "symfony/yaml": ">=3.3",
        "ramsey/uuid": ">=3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "mockery/mockery": "~1.0",
        "phpcompatibility/php-compatibility": "9.2.0"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "SilvertipSoftware\\Fixtures\\": "src/Fixtures/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/StubTestCase.php"
        ]
    },
    "scripts": {
        "ut": "phpunit",
        "cscompat-config": "phpcs --config-set installed_paths vendor/phpcompatibility/php-compatibility",
        "cscompat56": "phpcs --standard=PHPCompatibility --colors --runtime-set testVersion 5.6- --ignore=*.blade.php src tests"
    }
}
