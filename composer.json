{
    "name": "silvertipsoftware/fixtures",
    "description": "A better fixture package for Laravel 5",
    "keywords": ["laravel", "fixtures"],
    "type": "library",
    "license": "MIT",
    "version": "0.1.1",
    "authors": [
        {
            "name": "Silvertip Software",
            "email": "info@silvertipsoftware.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "illuminate/support": ">=5.5",
        "illuminate/database": ">=5.5",
        "illuminate/filesystem": ">=5.5",
        "symfony/yaml": ">=3.3",
        "ramsey/uuid": ">=3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "mockery/mockery": "~1.0"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "SilvertipSoftware\\Fixtures\\": "src/Fixtures/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/StubTestCase.php"
        ]
    }
}
